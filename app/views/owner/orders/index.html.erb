<div class="container">

    <h3><strong>注文一覧画面</strong></h3>
    <table class="table table-default">
        <tr>
            <td>購入日時</td>
            <td>購入者</td>
            <td>注文個数</td>
            <td>注文ステータス</td>
        </tr>
        <% @orders, @ordered_products.each do |order, ordered_product| %>
            <tr>
                <td>
                    <%= link_to owner_order_path(order) do %>
                        <%= order.created_at. strftime('%Y/%m/%d/%R') %>
                    <% end %>
                </td>
                <td><%= orders.member.first_name %><%= order.member.last_name %></td>
                <td><%= ordered_products.count %></td>
                <td><%= orders.status %></td>
            </tr>
        <% end %>
    </table>
</div>